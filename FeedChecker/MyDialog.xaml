<Window x:Class="FeedChecker.MyDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FeedChecker"
        mc:Ignorable="d"
        Title="Input" Height="112.722" Width="300.888">
    <Grid>
        <Button x:Name="btnOk" Content="Enter" HorizontalAlignment="Left" Margin="10,54,0,0" VerticalAlignment="Top" Width="75" Click="btnOk_Click"/>
        <TextBox x:Name="tBoxInput" Height="23" Margin="10,26,10,0" TextWrapping="Wrap" VerticalAlignment="Top" KeyDown="tBoxInput_KeyDown"/>
        <PasswordBox x:Name="tBoxPassword" Height="23" Margin="10,26,10,0" VerticalAlignment="Top" KeyDown="tBoxInput_KeyDown"/>
        <Label x:Name="labelMessage" Content="Label" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
