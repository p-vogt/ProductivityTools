<Window x:Class="StartupManager.Windows.FileFolderDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StartupManager.Windows"
        mc:Ignorable="d"
        Title="FileFolderDialog" Height="72.776" Width="199.73" ResizeMode="NoResize" SizeToContent="WidthAndHeight">
    <Grid>
        <Button x:Name="btnFolder" Content="Folder" HorizontalAlignment="Left" Margin="90,10,10,10" VerticalAlignment="Top" Width="75" Click="btnFolder_Click"/>
        <Button x:Name="btnFile" Content="File" HorizontalAlignment="Left" Margin="10" VerticalAlignment="Top" Width="75" Click="btnFile_Click"/>

    </Grid>
</Window>
